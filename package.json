{
  "name": "bunnyera-ai",
  "version": "1.0.0",
  "description": "BunnyERA AI – 三合一桌面工作流",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "gateway": "cd gateway && node index.js",
    "server": "node server.js",
    "web": "cd bunnyera-ai-web && npm run dev",
    "electron": "cd bunnyera-ai-web && npm run electron:dev",
    "dev": "concurrently \"npm run gateway\" \"npm run server\" \"npm run electron\""
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "body-parser": "^2.2.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "http-proxy-middleware": "^3.0.5",
    "jsonwebtoken": "^9.0.3",
    "node-telegram-bot-api": "^0.67.0",
    "openai": "^6.15.0",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "sqlite3": "^5.1.7",
    "ws": "^8.18.3"
  }
}
